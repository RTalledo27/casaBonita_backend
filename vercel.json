
{
  "version": 2,
  "name": "casa-bonita-api",
  "builds": [
    {
      "src": "index.php",
      "use": "vercel-php@0.6.0",
      "config": {
        "includeFiles": "**",
        "excludeFiles": [
          "node_modules/**",
          ".git/**",
          "tests/**",
          "storage/logs/**",
          "storage/framework/cache/**",
          "storage/framework/sessions/**",
          "storage/framework/views/**",
          ".env*",
          "*.md",
          "deploy.*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie",
    "CACHE_DRIVER": "array",
    "QUEUE_CONNECTION": "sync"
  },
  "regions": ["iad1"],
  "github": {
    "autoAlias": false
  }
}