{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.3",
      "config": {
        "includeFiles": "**",
        "excludeFiles": [
          "node_modules/**",
          ".git/**",
          "tests/**",
          "storage/logs/**",
          "storage/framework/cache/**",
          "storage/framework/sessions/**",
          "storage/framework/views/**",
          ".env*",
          "*.md",
          "deploy.*",
          "check_*.php",
          "debug_*.php",
          "test_*.php",
          "create_*.php",
          "list_*.php",
          "find_*.php",
          "generate_*.php",
          "read_*.php",
          "update_*.php",
          "verify_*.php",
          "diagnose_*.php"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "array",
    "CACHE_DRIVER": "array",
    "QUEUE_CONNECTION": "sync",
    "DB_CONNECTION": "sqlite",
    "DB_DATABASE": "/tmp/database.sqlite",
    "BROADCAST_CONNECTION": "log",
    "FILESYSTEM_DISK": "local"
  },
  "regions": ["iad1"],
  "github": {
    "autoAlias": false
  }
}